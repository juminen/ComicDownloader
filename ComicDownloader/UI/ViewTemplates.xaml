<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:v="clr-namespace:ComicDownloader.UI.View"
    xmlns:vm="clr-namespace:ComicDownloader.UI.ViewModel"
    xmlns:log="clr-namespace:JMI.General.View.WPF.Logging;assembly=JMI.General.View.WPF">
    <Style
        TargetType="{x:Type Rectangle}" />
    <DataTemplate
        DataType="{x:Type vm:ComicManagerViewModel}">
        <v:ComicManagerView />
    </DataTemplate>
    <DataTemplate
        DataType="{x:Type vm:ComicWorkspaceViewModel}">
        <v:ComicWorkspaceView />
    </DataTemplate>
    <DataTemplate
        DataType="{x:Type vm:ComicListItemViewModel}">
        <v:ComicListItemView />
    </DataTemplate>
    <DataTemplate
        DataType="{x:Type vm:ComicListViewModel}">
        <v:ComicListView />
    </DataTemplate>
    <DataTemplate
        DataType="{x:Type vm:CreateNewComicTabViewModel}">
        <v:CreateNewComicView />
    </DataTemplate>
    <DataTemplate
        DataType="{x:Type vm:EditComicTabViewModel}">
        <v:EditComicView />
    </DataTemplate>
    <DataTemplate
        DataType="{x:Type vm:DowloadLogTabViewModel}">
        <log:LogView />
    </DataTemplate>
</ResourceDictionary>